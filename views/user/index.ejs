<%- include('navbar_1.ejs')%>
<!-- about section -->

<section class="about_section layout_padding">
  <div class="container-fluid">
    <div class="box">
      <div class="img_container">
        <div class="img-box b1">
          <img src="/images/<%=about.image1%>" alt="" />
        </div>
        <div class="img-box b2">
          <img src="/images/<%=about.image2%>" alt="" />
        </div>
      </div>
      <div class="detail-box">
        <h2><%=about.heading%></h2>
        <p>
          <%=about.subheading%>
        </p>
        <a href=""> Read More </a>
      </div>
    </div>
  </div>
</section>
<!-- end about section -->

<!-- portfolio section -->

<section class="portfolio_section layout_padding-top">
  <div class="container">
    <div class="heading_container">
      <h2>Our Portfolio</h2>
      <p>There are many variations of</p>
    </div>
  </div>
  <div class="portfolio_container slick-carousel">
      <% for(var i = 0 ; i<port_list.length; i++){%>
        <div class="box">
          <img src="images/<%=port_list[i].image%>" alt="">
          <div class="link-box">
            <a href="<%=port_list[i].url%>">
              <img src="images/link.png" alt="">
            </a>
            <h6>
              <%=port_list[i].title%>
            </h6>
          </div>
        </div>
      <%}%>
    </div>
  </div>
</section>

<!-- end portfolio section -->

<!-- quality section -->

<section class="quality_section layout_padding" style="background-image: url('/images/<%=quality_info.bg_image%>');">
  <div class="container">
    <div class="quality_container">
      <h2>
        Quality <br />
        your Photo
      </h2>
      <div class="box">
        <div class="detail-box">
          <div class="img-box">
            <img src="/images/<%=quality_info.icon1%>" alt="" />
          </div>
          <div class="text-box">
            <h5><%=quality_info.heading1%></h5>
            <p><%=quality_info.subheading1%></p>
          </div>
        </div>
        <div class="detail-box">
          <div class="img-box">
            <img src="/images/<%=quality_info.icon2%>" alt="" />
          </div>
          <div class="text-box">
            <h5><%=quality_info.heading2%></h5>
            <p><%=quality_info.subheading2%></p>
          </div>
        </div>
      </div>
    </div>
  </div>
</section>

<!-- end quality section -->

<!-- client section -->

<section class="client_section layout_padding">
  <div class="container">
    <div class="heading_container">
      <h2>Testimonial</h2>
      <p>Lorem Ipsum available, but the majority</p>
    </div>
    <div class="client_container">
      <a
        class="carousel-control-prev"
        href="#carouselExampleControls"
        role="button"
        data-slide="prev"
      >
        <span class="sr-only">Previous</span>
      </a>
      <a
        class="carousel-control-next"
        href="#carouselExampleControls"
        role="button"
        data-slide="next"
      >
        <span class="sr-only">Next</span>
      </a>
      <div
        id="carouselExampleControls"
        class="carousel slide"
        data-ride="carousel"
      >
        <div class="carousel-inner">
          <% for(var i=0; i<test_list.length; i++){%>
          <div class="carousel-item <%= (i==0)?'active':'' %>">
          <div class="box">
            <div class="img-box">
              <img src="/images/<%=test_list[i].image%>" alt="" />
            </div>
            <div class="detail-box">
              <h4><%=test_list[i].title%>"</h4>
              <p>
              <%=test_list[i].description%>"
              </p>
              <img src="/images/quote.png" alt="" />
            </div>
          </div>
        </div>
          <%}%>
        </div>
      </div>
    </div>
  </div>
</section>

<!-- end client section -->

<!-- news section -->

<section class="news_section layout_padding">
  <div class="container">
    <div class="heading_container">
      <h2>Letest News</h2>
      <p>Lorem Ipsum available, but the majority</p>
    </div>
    <div class="row">
      <% for(var i=0; i<news_list.length; i++){%>
      <div class="col-md-6">
      <div class="box">
        <div class="img-box">
          <img src="/images/<%=news_list[i].image%>" alt="" />
          <div class="date">
            <h5><%=news_list[i].news_date%></h5>
          </div>
        </div>
        <div class="detail-box">
          <h5><%=news_list[i].title%></h5>
          <p><%=news_list[i].description%>
          </p>
        </div>
      </div>
    </div>
      <%}%>
    </div>
  </div>
</section>

<!-- end news section -->

<!-- contact section -->

<section class="contact_section layout_padding">
  <div class="container">
    <div class="">
      <h2 class="">Get In Touch</h2>
    </div>
  </div>
  <div class="container">
    <div class="row">
      <div class="col-md-6">
        <form action="/save_enquiry" method="post">
          <div>
            <input type="text" name="name" placeholder="Name" />
          </div>
          <div>
            <input type="email" name="email" placeholder="Email" />
          </div>
          <div>
            <input type="text" name="mobile" placeholder="Phone Number" />
          </div>
          <div>
            <input type="text" class="message-box" name="message" placeholder="Message" />
          </div>
          <div class="d-flex">
            <button>SEND</button>
          </div>
        </form>
      </div>
      <div class="col-md-6">
        <div class="img-box">
          <img src="/images/contact-img.png" alt="" />
          <div class="img_overlay">
            <h2>
              Best <br />
              Photo <br />
              studio
            </h2>
            <a href=""> Contact Us </a>
          </div>
        </div>
      </div>
    </div>
  </div>
</section>

<!-- end contact section -->

<%- include('footer.ejs')%>
