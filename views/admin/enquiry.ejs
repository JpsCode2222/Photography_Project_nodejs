<%- include('navbar.ejs')%>
<div class="container-fluid">
<!--  Row 1 -->
<div class="row">
  <div class="col-lg-12 d-flex align-items-strech">
    <div class="card w-100">
      <div class="card-body">
        <div class="d-sm-flex d-block align-items-center justify-content-between">
          <div class="mb-sm-0">
            <h5 class="card-title fw-semibold">Manage Enquiry</h5>
          </div>
        </div>
    
      <div class="row">
        <div class="col-lg-12 mt-5">
          <table class="table table-responsive table-sm table-striped table-hover table-bordered">
            <tr>
              <th>Action</th>
              <th>SN</th>
              <th>Date</th>
              <th>Name</th>
              <th>Email</th>
              <th>Mobile</th>
              <th>Message</th>
            </tr>
            <% for(var i=0; i<enquiry_list.length; i++){%>
                <tr>
                    <td style="white-space: nowrap;">
                        <a href="/admin/deactive_enquiry/<%=enquiry_list[i]._id%>"><i class="ti ti-check btn btn-sm btn-primary"></i></a>
                    </td>
                    <td><%=i+1%></td>
                    <td><%=enquiry_list[i].date%></td>
                    <td><%=enquiry_list[i].name%></td>
                    <td><%=enquiry_list[i].email%></td>
                    <td><%=enquiry_list[i].mobile%></td>
                    <td><%=enquiry_list[i].message%></td>
                </tr>
            <%}%>
          </table>
        </div>
      </div>
      </div>
    </div>
  </div>
</div>
</div>

<%- include('footer.ejs')%>